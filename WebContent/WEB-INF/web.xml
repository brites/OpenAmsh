    <display-name>ACTReport</display-name>
    <servlet-name>ACTReport</servlet-name>
    <servlet-class>com.fatec.openamsh.server.view.ACTReport</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ACTReport</servlet-name>
    <url-pattern>/ACTReport</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>ACTReport1</display-name>
    <servlet-name>ACTReport1</servlet-name>
    <servlet-class>com.fatec.openamsh.server.view.ACTReport1</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>ACTReport1</servlet-name>
    <url-pattern>/ACTReport1</url-pattern>
